{
    "name": "basilisk/modelfilter",
    "description": "ModelFilter provides an easy way to filter Eloquent Models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sahand khodaparast",
            "email": "sahand.khodaparast@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Basilisk\\ModelFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Basilisk\\ModelFilter\\ModelFilterServiceProvider"
            ],
            "aliases": {
                "ModelFilter": "Basilisk\\ModelFilter\\Facades\\ModelFilterFacade"
            }
        }
    }
}
